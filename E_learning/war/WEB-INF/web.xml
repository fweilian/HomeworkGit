<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<servlet>
		<servlet-name>E_learning</servlet-name>
		<servlet-class>fan.gae.learning.E_learningServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>E_learning</servlet-name>
		<url-pattern>/e_learning</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>fan.gae.learning.user.loginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>editUserInfo</servlet-name>
		<servlet-class>fan.gae.learning.user.EditUserInfoServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>editUserInfo</servlet-name>
		<url-pattern>/editUserInfo</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>ApplyTeacher</servlet-name>
		<servlet-class>fan.gae.learning.user.ApplyTeacherServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ApplyTeacher</servlet-name>
		<url-pattern>/applyTeacher</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>ApplyTeacherHandle</servlet-name>
		<servlet-class>fan.gae.learning.user.ApplyTeacherHandleServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ApplyTeacherHandle</servlet-name>
		<url-pattern>/applyTeacherHandle</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>EditTeacherInfo</servlet-name>
		<servlet-class>fan.gae.learning.user.EditTeacherInfoServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EditTeacherInfo</servlet-name>
		<url-pattern>/editTeacherInfo</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>ApplyCourse</servlet-name>
		<servlet-class>fan.gae.learning.course.ApplyCourseServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ApplyCourse</servlet-name>
		<url-pattern>/applyCourse</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>UploadCourse</servlet-name>
		<servlet-class>fan.gae.learning.course.UploadCourseServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UploadCourse</servlet-name>
		<url-pattern>/uploadCourse</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>UploadFile</servlet-name>
		<servlet-class>fan.gae.learning.upload.UploadServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UploadFile</servlet-name>
		<url-pattern>/uploadFile</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>UploadVideo</servlet-name>
		<servlet-class>fan.gae.learning.upload.UploadVideoServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UploadVideo</servlet-name>
		<url-pattern>/uploadVideo</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Serve</servlet-name>
		<servlet-class>fan.gae.learning.upload.ServeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Serve</servlet-name>
		<url-pattern>/serve</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>CreateGroup</servlet-name>
		<servlet-class>fan.gae.learning.discuss.CreateGroupServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CreateGroup</servlet-name>
		<url-pattern>/createGroup</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
